!!! Strict
%html
  %head
    %title aether
    %link{:href=>"/stylesheets/screen.css", :media=>"screen", :rel=>"stylesheet", :type=>"text/css"}
    %link{:href=>"/stylesheets/jquery-ui.css", :media=>"screen", :rel=>"stylesheet", :type=>"text/css"}
    %script{ :src => "http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js", :type => "text/javascript"}
    %script{:src => "/javascript/jquery.ui.min.js", :type => "text/javascript"}
    %script{:src => "http://maps.google.com/?file=api&amp;v=2.x&key=ABQIAAAANhns9HfwQK2FOiJknFe_RBQ1bmiFIOX-FukFLYVl-b0g3SpkNRQKTD1S460t8SmwYB44diqyQepOOQ", :type => "text/javascript"}
    %script{:src => "/javascript/markermanager_packed.js", :type => "text/javascript"}
    %script{:src => "/javascript/jquery.tablesorter.js", :type => "text/javascript"}
    %script{:src => "/javascript/jquery.tablesorter.pager.js", :type => "text/javascript"}
    %script{:src => "/javascript/jquery.dotimeout.min.js", :type => "text/javascript"}
    %script{:src => "/javascript/jquery.debug.js", :type => "text/javascript"}
    %script{:src => "/javascript/callbacks.js", :type => "text/javascript"}
    %script{:src => "/javascript/aether.js", :type => "text/javascript"}
  %body
    .container
      #header
        %img{:src => "images/header.jpg"}
      #left_bar
        %div#navigation{:class => 'ui-accordion ui-widget ui-helper-reset'}
          %h3{:class => 'ui-accordion-header ui-helper-reset ui-corner-top'}
            %span{:class => 'ui-icon ui-icon-triangle-1-e'}
            %a{:href => "#"} Airports
          %div{:class => 'ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active'}
            %a{:href => "#", :id => 'AllAirportsInCountry'} Airports by Country
            %br/
            %a{:href => "#", :id => 'Elevation'} Airports by Elevation
            %br/
            %a{:href => "#", :id => 'AirportsAirlineServices'} Airports by Airline
            %br/
            %a{:href => "#", :id => 'CostBetweenAirports'} Destinations from Airport (Direct Only)
            %br/
            %a{:href => "#", :id => 'AirportDistance'} Distance Between Airports
            %br/
            %a{:href => "#", :id => 'AirportTimeDifferences'} Time Change Between Airports
            %br/
            %a{:href => "#", :id => 'AllAirports'} All Airports
          %h3{:class => 'ui-accordion-header ui-helper-reset ui-state-default ui-corner-all'}
            %span{:class => 'ui-icon ui-icon-triangle-1-e'}
            %a{:href => "#"} Airlines
          %div{:class => 'ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom'}
            %a{:href => "#", :id => 'AirlinesLeavingAirport'} Airlines by Airport (Departing)
            %br/
            %a{:href => "#", :id => 'AirlinesEnteringAirport'} Airlines by Airport (Arriving)
            %br/
            %a{:href => "#", :id => 'AllAirlines'} All Airlines
          %h3{:class => 'ui-accordion-header ui-helper-reset ui-state-default ui-corner-all'}
            %span{:class => 'ui-icon ui-icon-triangle-1-e'}
            %a{:href => "#"} Flights
          %div{:class => 'ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom'}
            %a{:href => "#", :id => 'RoutesAirlineServices'} Flights by Airline
            %br/
            %a{:href => "#", :id => 'ShortestFlight'} Shortest flight
            %br/
            %a{:href => "#", :id => 'CheapestFlight'} Cheapest flight
            %br/
            %a{:href => "#", :id => 'CostBetweenAirports'} All flights between airports by price
            %br/
            %a{:href => "#", :id => 'AllRoutes'} All Flights
      #content
        #mapOuter
          %ul.topNav
            %li
              %a{:href => "#", :alt => "Click to toggle", :id => 'mapToggle'}
                Toggle Map
            %li
              |
            %li
              %a{:href => "#", :alt => "Click to scroll", :id => 'toTable'}
                Jump to Table
          #map
        #results
          #loading
            %img{:src => "images/loader.gif"}
            #progress
          #pager.pager
            %form
              %span.first &laquo;
              %input.pagedisplay{:type => "text", :text => "1/1"}
              %span.prev &lsaquo;
              %span.next &rsaquo;
              %span.last &raquo;
              %select.pagesize#pagesize{:name => "pagesize"}
                %option{:value => 10} 10
                %option{:value => 20} 20
                %option{:value => 30} 30
                %option{:value => 40} 40
                %option{:selected => true, :value => 50} 50
                %option{:value => 100} 100
          %table#data
            %thead
            %tbody
          %a{:href => "#", :alt => "Click to scroll", :id => 'toTop'} Top
    #dialog
    :javascript
      $(document).ready(function(){
        load();
      });
      $(window).unload(function(){
        unload();
      });
          